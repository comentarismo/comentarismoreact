<!DOCTYPE html>
<html>
<head>
    <title>Welcome to Comentarismo</title>
    <% styleSrc.forEach(function(src){ %>
    <link rel="stylesheet" href="<%= src %>" type="text/css" media="screen" charset="utf-8">
    <% }) %>

</head>
<body>
<div id="root"><%- html %></div>
<script type="text/javascript" charset="utf-8">
    window.__REDUX_STATE__ = '<%= reduxState %>';
</script>

<% scriptSrcs.forEach(function(src){ %>
<script src="<%= src %>"></script>
<% }) %>

</body>
</html>

